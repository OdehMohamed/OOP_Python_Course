from abc import ABC, abstractmethod

class ProgrammingLnguage:
  @abstractmethod
  def is_oop(self):
    pass
class Cpp(ProgrammingLnguage):
  pass


obj=ProgrammingLnguage()
print(obj.is_oop())

obj2=Cpp()


